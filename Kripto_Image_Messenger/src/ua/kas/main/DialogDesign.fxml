<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="150.0" prefWidth="300.0" styleClass="root" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ua.kas.main.Controller">
   <children>
      <TextField fx:id="address" layoutX="35.0" layoutY="63.0" prefHeight="25.0" prefWidth="230.0" promptText="Please enter address!" style="-fx-background-color: #fc97c9;">
         <font>
            <Font name="Times New Roman" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutY="-10.0" prefHeight="73.0" prefWidth="300.0" text="KriptoImageMessenger" textAlignment="CENTER" textFill="AQUA">
         <font>
            <Font name="AR DELANEY" size="29.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <Button layoutX="35.0" layoutY="99.0" mnemonicParsing="false" onAction="#host" prefHeight="25.0" prefWidth="100.0" text="Host">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="161.0" layoutY="99.0" mnemonicParsing="false" onAction="#play" prefHeight="25.0" prefWidth="100.0" text="User">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
